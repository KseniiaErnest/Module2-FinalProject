<section class="margin-top-body">
 
 <h1>Your notes for this Flashcard</h1>

 <div class="all-notes-container">



  {{#each notes}}
  <div >
    <div class="each-note-container">
  <h2>{{this.title}}</h2>
  <p>{{this.content}}</p>

  <div class="btn-container-notes">
  <button class="btn-notes"><a href="/note/edit-note/{{this._id}}"> Update This Note</a></button>

  <form action="/note/{{this._id}}/delete-note" method="POST">
    <button class="btn-notes"> Delete This Note</button>
  </form>
  </div>

</div>

</div>
  {{/each}}



</div>

</section>

