<h1 class="homepage-heading">The Name of The App</h1>

<h2>Kanji Search</h2>

<form action="/" method="GET">
 <input type="text" name="q" placeholder="Enter a search">
    <button type="submit">Search</button>
</form>

{{#if result}}
    <h2>Search Results:</h2>
    {{#if result.length}}
      <ul>
        {{#each result}}
          <li>{{this.kanji.character}}</li>
        {{/each}}
      </ul>
    {{else}}
      <p>No results found.</p>
    {{/if}}
  {{/if}}

